{
    "stac_version": "0.9.0",
    "stac_extensions": [
        "sar",
        "asset",
        "commons"
    ],
    "id": "sentinel-s1-l2",
    "title": "Sentinel-1 L2",
    "description": "Sentinel-1A and Sentinel-1B processed data",
    "keywords": [
        "sentinel",
        "earth observation",
        "esa",
        "sar"
    ],
    "extent": {
        "spatial": [
            -180,
            -90,
            180,
            90
        ],
        "temporal": [
            "2013-06-01",
            null
        ]
    },
    "providers": [
        {
            "name": "ESA",
            "roles": ["producer"],
            "url": "https://earth.esa.int/web/guest/home"
        },
        {
            "name": "Capella Space",
            "roles": ["processor"],
            "url": "https://www.capellaspace.com"
        },
        {
            "name": "AWS",
            "roles": ["host"],
            "url": "http://sentinel-pds.s3-website.eu-central-1.amazonaws.com/"
        },
        {
            "name": "Element 84",
            "roles": ["processor"],
            "url": "https://element84.com"
        }
    ],
    "license": "proprietary",
    "properties": {
        "constellation": "sentinel-1",
        "instruments": ["c-sar"],
        "sar:frequency_band": "C",
        "sar:polarizations": ["HH", "HV", "VV", "VH"],
        "sar:center_frequency": 5.405,
        "sar:resolution_range": 50,
        "sar:resolution_azimuth": 50,
        "sar:pixel_spacing_range": 25,
        "sar:pixel_spacing_azimuth": 25,
        "sar:observation_direction": "right"
    },
    "summaries": {
        "platform": ["sentinel-1a", "sentinel-1b"]
    },
    "item_assets": {
        "thumbnail": {
            "title": "Thumbnail",
            "type": "image/png",
            "roles": ["thumbnail"]
        },
        "preview": {
            "title": "Preview",
            "description": "Full resolution byte-scaled COG preview",
            "type": "image/tiff; application=geotiff cloud-optimized=true",
            "roles": ["overview"]
        },
        "metadata": {
            "title": "Metadata",
            "type": "application/json",
            "roles": ["metadata"]
        },
        "HH": {
            "title": "HH",
            "description": "HH Polarization",
            "type": "image/tiff; application=geotiff",
            "roles": ["data"],
            "sar:polarizations": ["HH"]
        },
        "HH-metadata": {
            "title": "HH Metadata",
            "description": "HH Polarization metadata",
            "type": "application/xml",
            "roles": ["metadata"]
        },
        "VV": {
            "title": "VV",
            "description": "VV Polarization",
            "type": "image/tiff; application=geotiff",
            "roles": ["data"],
            "sar:polarizations": ["VV"]
        },
        "VV-metadata": {
            "title": "VV Metadata",
            "description": "VV Polarization metadata",
            "type": "application/xml",
            "roles": ["metadata"]
        },
        "HV": {
            "title": "HV",
            "description": "HV Polarization",
            "type": "image/tiff; application=geotiff",
            "roles": ["data"],
            "sar:polarizations": ["HV"]
        },
        "HV-metadata": {
            "title": "HV Metadata",
            "description": "HV Polarization metadata",
            "type": "application/xml",
            "roles": ["metadata"]
        },
        "VH": {
            "title": "VH",
            "description": "VH Polarization",
            "type": "image/tiff; application=geotiff",
            "roles": ["data"],
            "sar:polarizations": ["VH"]
        },
        "VH-metadata": {
            "title": "VH Metadata",
            "description": "VH Polarization metadata",
            "type": "application/xml",
            "roles": ["metadata"]
        }
    },
    "links": [
        {
            "rel": "license",
            "href": "https://sentinel.esa.int/documents/247904/690755/Sentinel_Data_Legal_Notice"
        },
        {
            "rel": "about",
            "href": "https://github.com/sat-utils/sat-stac-sentinel"
        }
    ]
}
